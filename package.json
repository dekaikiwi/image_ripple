{
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "8",
    "browserify": "^16.5.0",
    "jquery": "^3.4.1",
    "jquery.ripples": "^0.6.2",
    "watchify": "^3.11.1"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "scripts": {
    "build": "browserify app.js -o dist/bundle",
    "watch": "watchify app.js -o dist/bundle -v"
  },
  "version": "0.0.0"
}
